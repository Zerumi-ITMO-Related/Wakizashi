val b: Int = 14;

fun main() : Unit {
    val a: Int = 10;
    val b: Int = factorial(a) + 12;
    val c: String = "Hello, World!";
    print(a);
    print(b);
    print(c);
}

fun factorial(n: Int) : Int {
    if (n == 0) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}
